#include <stdio.h>

int main() {
    int rows = 6;
    int cols = 4;
    int arr[6][4];
    int i, j;

    int value = 1;
    for (i = 0; i < rows; i++) {
        for (j = 0; j < cols; j++) {
            arr[i][j] = value;
            value++;
        }
    }

    printf("2D Array elements:\n");
    for (i = 0; i < rows; i++) {
        for (j = 0; j < cols; j++) {
            printf("%d\t", arr[i][j]);
        }
        printf("\n");
    }

    return 0;
}
